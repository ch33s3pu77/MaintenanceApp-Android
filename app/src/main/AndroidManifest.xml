<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.maintenanceapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".output.NewJobs" />
        <activity android:name=".output.NotStarted" />
        <activity android:name=".output.InProgress" />
        <activity android:name=".output.Completed" />
        <activity android:name=".input.SignOff" />
        <activity android:name=".input.PutOnHold" />
        <activity android:name=".input.NewJob" />
        <activity android:name=".output.JobOrder" />
        <activity android:name=".output.OnHold" />
        <activity android:name=".output.MyJobs" />
        <activity android:name=".Dashboard" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>